import java.util.Scanner;

public class P7_1 {

	private static Scanner a;
	private static Scanner b;
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int ak = 0;
		char bp;
		a = new Scanner(System.in);
        System.out.print("Enter the number of students:");
        int stuNum = a.nextInt();
        int[] score = new int[100];
        System.out.print("Enter " + stuNum + " scores:");
        for(int i=0;i<stuNum;i++) {
    	b = new Scanner(System.in);
    	score[i] = b.nextInt();
    	System.out.println("Student " + i + " score is " + score[i] + " and grade is " + );
        }
        for(int i=0;i<stuNum;i++) {
        if(score[stuNum-i-1]>score[i]) {
        	ak = score[stuNum-i-1];
        }else {
        	ak = score[i];    	
        } 
        if(score[i]>=ak-10) {
        	bp = 'A';
        }else if(score[i]>=ak-20) {
        	bp = 'B';
        }else if(score[i]>=ak-30) {
        	bp = 'C';
        }else if(score[i]>=ak-40) {
        	bp = 'D';
        }
	}
	}

}
