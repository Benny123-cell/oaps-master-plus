		goal[i] = a.nextInt();
		if(goal[i]==0)
			break;
		doal[goal[i]]++;
	}
	for(int i=0;i<1005;i++) {
		if(doal[i]==1)
			System.out.println(i + " occurs " + doal[i] + " times");
		else if(doal[i]>1)
			System.out.println(i + " occurs " + doal[i] + " times");
	}
	}
